{
    "name": "vulnerable/dummy-traversal",
    "description": "PHPUnit and Infection testing for path traversal vulnerabilities.",
    "type": "project",
    "license": "MIT",
    "require-dev": {
        "phpunit/phpunit": "^9.5 || ^10.0 || ^11.0",
        "infection/infection": "^0.27"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --testdox",
        "infection": "infection"
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
